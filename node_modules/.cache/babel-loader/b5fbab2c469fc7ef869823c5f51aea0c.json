,GAAA,KAAA;AACA,KAvBA;AAwBA,IAAA,YAxBA,wBAwBA,KAxBA,EAwBA;AACA,UAAA,QAAA,GAAA,KAAA,iBAAA,CAAA,OAAA,CAAA,KAAA,KAAA,CAAA,KAAA,EAAA,MAAA,CAAA;AACA,UAAA,EAAA,QAAA,GAAA,CAAA,EAAA,QAAA,GAAA,CAAA;AACA,WAAA,KAAA,CAAA,KAAA,EAAA,MAAA,GAAA,KAAA,iBAAA,CAAA,QAAA,CAAA;AACA,KA5BA;AA6BA,IAAA,cA7BA,0BA6BA,GA7BA,EA6BA;AACA,aAAA,GAAA,CAAA,MAAA,CAAA,CAAA,EAAA,WAAA,KAAA,GAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,KA/BA;AAgCA,IAAA,cAhCA,4BAgCA;AACA,UAAA,GAAA,GAAA,IAAA,CAAA,SAAA,CAAA,KAAA,KAAA,CAAA;AACA,MAAA,YAAA,CAAA,OAAA,CAAA,eAAA,EAAA,GAAA;AACA;AAnCA,GAbA;AAkDA,EAAA,OAlDA,qBAkDA;AACA,QAAA,QAAA,GAAA,YAAA,CAAA,OAAA,CAAA,eAAA,CAAA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,OAAA,EAAA,QAAA;AACA,QAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,QAAA,CAAA;AACA,SAAA,KAAA,GAAA,IAAA;AACA;AAvDA,CAAA","sourcesContent":["<template>\n  <div class=\"container\">\n    <h2 class=\"text-center mt-5\">My Vue Todo App</h2>\n    <!-- Input -->\n    <div class=\"d-flex\">\n      <input v-model=\"task\" type=\"text\" placeholder=\"Enter task\" class=\"form-control\">\n      <button @click=\"submitTask\" class=\"btn btn-warning rounded-0\">SUBMIT</button>\n    </div>\n    <!-- Task table -->\n    <table class=\"table table-bordered mt-5\">\n  <thead>\n    <tr>\n      <th scope=\"col\">Task</th>\n      <th scope=\"col\">Status</th>\n      <th scope=\"col\" class=\"text-center\">#</th>\n      <th scope=\"col\" class=\"text-center\">#</th>\n    </tr>\n  </thead>\n  <tbody>\n    <tr v-for=\"(task, index) in tasks\" :key=\"index\">\n      <td>\n        <span :class=\"{'finished': task.status === 'finished'}\">{{task.name}}</span>\n      </td>\n      <td><span @click=\"changeStatus(index)\" class=\"pointer\">\n        {{firstCharUpper(task.status)}}\n        </span></td>\n      <td>\n        <div class=\"text-center\" @click=\"editTask(index)\">\n          <span class=\"fa fa-pen\"></span>\n        </div>\n      </td>\n      <td>\n        <div class=\"text-center\" @click=\"deleteTask(index)\">\n          <span class=\"fa fa-trash\"></span>\n        </div>\n      </td>\n    </tr>\n  </tbody>\n</table>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'HelloWorld',\n  props: {\n    msg: String\n  },\n  data() {\n    return {\n      task: '',\n      editedTask : null,\n      availableStatuses: ['to-do', 'in-progress', 'finished'],\n      tasks: []\n    }\n  },\n  methods: {\n    submitTask() {\n      if(this.task.length === 0) return;\n\n      if(this.editedTask === null) {\n        this.tasks.push({\n        name: this.task,\n        status: 'to-do'\n      });\n      }else {\n        this.tasks[this.editedTask].name = this.task;\n        this.editedTask = null;\n      }\n      this.task = ''\n      this.saveToDatabase();\n      \n    },\n    deleteTask(index){\n      this.tasks.splice(index, 1)\n    },\n    editTask(index) {\n      this.task = this.tasks[index].name;\n      this.editedTask = index;\n    },\n    changeStatus(index) {\n      let newIndex = this.availableStatuses.indexOf(this.tasks[index].status);\n      if(++newIndex > 2) newIndex = 0;\n      this.tasks[index].status = this.availableStatuses[newIndex];\n    },\n    firstCharUpper(str) {\n      return str.charAt(0).toUpperCase() + str.slice(1)\n    },\n    saveToDatabase(){\n      const str = JSON.stringify(this.tasks);\n      localStorage.setItem(\"todo-database\", str)\n    }\n  },\n  mounted() {\n    const rawValue = localStorage.getItem(\"todo-database\");\n    console.log(\"raw: \", rawValue)\n    const data = JSON.parse(rawValue);\n    this.tasks = data\n  },\n}\n</script>\n\n<style scoped>\n  .pointer {\n    cursor: pointer;\n  }\n  .finished {\n    text-decoration: line-through;\n  }\n</style>\n"],"sourceRoot":"src/components"}]}